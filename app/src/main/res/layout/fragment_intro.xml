<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mdimitrov.wunderchild.utils.ui.fragments.IntroFragment">

    <ImageView
        android:id="@+id/text_picture"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/text_picture_height"
        />

    <ImageView
        android:id="@+id/cildren_picture"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/children_picture_height" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/hello_blank_fragment" />

</FrameLayout>
